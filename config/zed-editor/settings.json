{
  "agent": {
    "default_model": {
      "model": "claude-3.7-sonnet",
      "provider": "copilot_chat"
    }
  },
  "auto_install_extensions": {
    "nvim-nightfox": true
  },
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_size": 15,
  "buffer_font_weight": 500,
  "command_aliases": {
    "Q": "q",
    "W": "w",
    "Wq": "wq"
  },
  "cursor_blink": false,
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "relative_line_numbers": true,
  "tab_bar": {
    "show": false
  },
  "theme": {
    "dark": "Carbonfox - opaque",
    "light": "Ayu Light",
    "mode": "dark"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 16,
  "vertical_scroll_margin": 0,
  "vim_mode": true
}
