{
  "coc.source.word.shortcut": "W",
  "coc.source.word.priority": 10,
  "suggest.enablePreview": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.minTriggerInputLength": 2,
  "diagnostic.enable": true,
  "diagnostic.warningSign": "●",
  "diagnostic.errorSign": "●",
  "diagnostic.infoSign": "●",
  "diagnostic.level": "hint",
  "eslint.autoFixOnSave": true,
  "snippets.priority": 100,
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.userSnippetsDirectory": "~/.dotfiles/vim/ultisnips",
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "javascript.suggestionActions.enabled": false,
  "languageserver": {
    "lua": {
      "command": "lua-lsp",
      "filetypes": ["lua"]
    },
    "clojure-lsp": {
      "command": "clojure-lsp",
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "shadown-cljs.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "initializationOptions": {}
    }
  },
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "list.height": 20,
  "list.matchHighlightGroup": "GruvboxRedBold"
}
