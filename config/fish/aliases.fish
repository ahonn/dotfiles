# tmux & tmuxinator
alias hello 'tmuxinator start world -n ahonn'
alias world 'tmuxinator start world -n'
alias bye 'tmux kill-server'
alias kill 'tmux kill-session -t'

# dotfiles
alias dot 'cd $DOTFILES'

# neovim
alias vim 'nvim'

# git
alias gst 'git status'
alias gch 'git checkout'

# better command
alias rm   'trash'
alias cat  'bat'
alias find 'fd'
