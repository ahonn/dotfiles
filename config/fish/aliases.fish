# tmux & tmuxinator
alias hello 'tmuxinator start ahonn'
alias world 'tmuxinator start project -n'
alias bye   'tmux kill-server'

# dotfiles
alias dot 'cd $DOTFILES'

# neovim
alias vim 'nvim'

# git
alias gst 'git status'
alias gch 'git checkout'

# safe remove
alias rm 'trash'

# better cat
alias cat 'bat'
