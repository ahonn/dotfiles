{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },

  "extends": "airbnb-base",

  "rules": {
    "semi": 0,
    "camelcase": 0,
    "arrow-parens": 0,
    "no-unused-vars": 1
  }
}
